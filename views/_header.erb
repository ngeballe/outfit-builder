<header class="page-header">
  <a href="/" class="logo">
    <h1 class="logo-heading">Outfit Builder</h1>
  </a>

  <div class="page-header-content"></div>

  <nav class="header-nav">
    <ul class="header-nav-ul">
      <% request_path = request.env['REQUEST_PATH'] %>
      <% @header_nav_links.each do |link| %>
        <li>
          <a href="<%= link[:href] %>" class="header-nav-link <%= active_nav_link_class(link, request_path) %>">
            <%= link[:name] %>
          </a>
        </li>
      <% end %>
    </ul>
  </nav>
</header>